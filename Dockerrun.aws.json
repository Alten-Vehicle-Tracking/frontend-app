{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "vehicle-tracking",
      "host": {
        "sourcePath": "/usr/src/app"
      }
    },
    {
      "name": "nginx-conf",
      "host": {
        "sourcePath": "/etc/nginx"
      }
    }
  ],
  "workingDirectory": "/usr/src/app",
  "containerDefinitions": [
    {
      "name": "frontend",
      "image": "altenchallenge/frontend:latest",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80,
          "protocol": "tcp"
        }
      ],
      "mountPoints": [
        {
          "sourceVolume": "vehicle-tracking",
          "containerPath": "/usr/src/app",
          "readOnly": true
        },
        {
          "sourceVolume": "nginx-conf",
          "containerPath": "/etc/nginx",
          "readOnly": true
        },
        {
          "sourceVolume": "awseb-logs-vehicle-tracking",
          "containerPath": "/var/log/vehicle-tracking"
        }
      ]
    }
  ]
}
